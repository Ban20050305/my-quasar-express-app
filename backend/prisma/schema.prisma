// prisma/schema.prisma


datasource db {
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
}



model Task {
  id          String   @id @default(uuid())
  title       String
  description String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt

  @@map("tasks")
}
